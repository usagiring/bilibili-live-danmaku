{
  "name": "bilibili-danmaku",
  "version": "0.4.21",
  "author": "其妙 <hling51325@163.com>",
  "description": "这是一个哔哩哔哩的直播弹幕应用",
  "license": "MIT",
  "main": "dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:all": "npm run build:danmaku && npm run build:danmaku-scroll && node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "",
    "ui": "vue ui",
    "release": "npm run build -- -p always",
    "cnpmi": "npm --registry https://registry.npm.taobao.org install",
    "build:danmaku": "cd danmaku && npm run build:monorepo",
    "build:danmaku-scroll": "cd danmaku-scroll && npm run build:monorepo"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/usagiring/bilibili-live-danmaku.git"
  },
  "build": {
    "productName": "bilibili-danmaku",
    "appId": "com.electron.bilibili-danmaku",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "extraFiles": [
      "config.yaml"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "icon": "build/icons"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "publish": [
      {
        "provider": "github"
      }
    ]
  },
  "overrides": {
    "webpack": "5.74.0"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@tokine/bilibili-bridge": "^0.1.22",
    "@tokine/bilibili-recorder": "^0.0.11",
    "axios": "^0.21.2",
    "cookie": "^0.5.0",
    "echarts": "^5.3.3",
    "echarts-wordcloud": "^2.0.0",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.2.1",
    "flv.js": "^1.6.2",
    "font-list": "^1.4.5",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "view-ui-plus": "^1.3.1",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5",
    "vue3-smooth-dnd": "^0.0.2",
    "vuex": "^4.0.2",
    "vuex-electron": "github:usagiring/vuex-electron",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-function-bind": "^7.18.9",
    "@babel/plugin-proposal-json-strings": "^7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@babel/plugin-proposal-throw-expressions": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/register": "^7.18.9",
    "@types/node": "^20.4.5",
    "babel-loader": "^8.2.5",
    "cfonts": "^3.1.1",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^20.1.1",
    "electron-builder": "^23.3.3",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.46.0",
    "eslint-plugin-vue": "^9.4.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "2.6.1",
    "multispinner": "^0.2.1",
    "node-loader": "^2.0.0",
    "node-sass": "^7.0.1",
    "prettier": "^2.7.1",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "typescript": "^4.7.4",
    "url-loader": "^4.1.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^17.0.0",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.7.10",
    "vue-tsc": "^0.40.1",
    "webpack": "5.74.0",
    "webpack-dev-server": "^4.10.1",
    "webpack-hot-middleware": "^2.25.2",
    "worklet-loader": "^2.0.0"
  }
}
